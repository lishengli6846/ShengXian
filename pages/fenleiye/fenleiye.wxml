<view class='bg-img'>
  <image src='../../images/j-i1.jpg'></image>
</view>
<view class='wp'>
  <view class='share'>
    <button open-type='share'>{{share}}</button>
  </view>
  <view class='info'>
    <view class='btn'>
      <view wx:for="{{mesbtn}}" wx:key="{{index}}" data-index='{{index}}' bindtap='showCategory'>
        <navigator class='{{item.className}}' >
          {{item.text}}
        </navigator>
      </view>
    </view>
    <view class='txt'>
      {{words}}
    </view>
  </view>
</view>
